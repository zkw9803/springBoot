<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cykj.mapper.StudentMapper">
<!--    登录-->
    <select id="checkLog" resultType="Student">
        select * from student where username=#{username} and password=#{password};
    </select>
<!--    注册-->
    <insert id="checkReg" parameterType="Student">
        insert  into student (username,password) values (#{username},#{password})
    </insert>
<!--查询-->
    <select id="stuSe" resultType="Student">
        select * from student;
    </select>
</mapper>
